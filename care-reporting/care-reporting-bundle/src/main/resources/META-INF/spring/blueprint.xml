<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:osgi="http://www.eclipse.org/gemini/blueprint/schema/blueprint"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
       http://www.eclipse.org/gemini/blueprint/schema/blueprint http://www.eclipse.org/gemini/blueprint/schema/blueprint/gemini-blueprint.xsd">


	<import resource="classpath:META-INF/motech/applicationCareReporting.xml" />

	<osgi:reference id="eventRelayOsgi" cardinality="0..1"
		interface="org.motechproject.event.listener.EventRelay" />

	<osgi:reference id="eventListenerRegistry"
		interface="org.motechproject.event.listener.EventListenerRegistryService" />

	<osgi:reference id="platformSettingsService"
		interface="org.motechproject.server.config.service.PlatformSettingsService" />

	<osgi:reference id="schedulerServiceOsgi"
		interface="org.motechproject.scheduler.service.MotechSchedulerService"
		cardinality="0..1" />

	<osgi:reference id="schedulerDiagnosticsServiceOsgi"
		interface="org.motechproject.commcare.provider.sync.diagnostics.scheduler.SchedulerDiagnosticsService" />

	<osgi:reference id="mdsServiceFactory" 
		interface="org.motechproject.mcts.care.common.mds.service.MdsServiceFactory" />

</beans>